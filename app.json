{
  "expo": {
    "name": "Vitae",
    "slug": "Vitae",
    "platforms": ["ios"],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vitae",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1F23"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gobelait.Vitae",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "Cette application utilise l'audio pour la méditation et la relaxation.",
        "NSUserTrackingUsageDescription": "Vitae utilise l’identifiant publicitaire pour afficher une bannière discrète qui aide à financer l’application.",
        "UIBackgroundModes": ["audio"],
        "AVAudioSessionCategory": "playback",
        "AVAudioSessionMode": "default",
        "AVAudioSessionCategoryOptions": [
          "allowBluetooth",
          "allowBluetoothA2DP",
          "mixWithOthers"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-av",
        {
          "microphonePermission": "Cette application utilise l'audio pour la méditation et la relaxation."
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-7483950421454182~3017038089",
          "androidAppId": "ca-app-pub-3940256099942544~3347511713"
        }
      ]
    ],
    "assetBundlePatterns": ["assets/sounds/*"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "adsEnabled": true,
      "premium": {
        "iosProductId": "premium_monthly_ios",
        "priceMonthlyEUR": 1.99
      },
      "router": {},
      "eas": {
        "projectId": "ca93dabf-6bde-418c-b65f-076550a44211"
      }
    },
    "owner": "gobelait",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ca93dabf-6bde-418c-b65f-076550a44211",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    }
  }
}
